<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<TitledPane fx:id="titledPane" animated="false" collapsible="false" text="Overskrift" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.FunctionTemplateController">
   <content>
      <VBox minHeight="-Infinity" spacing="20.0">
         <children>
            <HBox spacing="20.0">
               <children>
                  <VBox prefWidth="200.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Nuværende niveau" />
                        <Label fx:id="lblCurrentLevel" />
                     </children>
                  </VBox>
                  <VBox prefWidth="200.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Forventet niveau" />
                        <Label fx:id="lblExpectedLevel" />
                     </children>
                  </VBox>
                  <VBox prefWidth="600.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Fagligt notat" />
                        <Label fx:id="lblNote" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox spacing="20.0">
               <children>
                  <VBox prefWidth="200.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Udførelse" />
                        <Label fx:id="lblExecution" />
                     </children>
                  </VBox>
                  <VBox prefWidth="200.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Betydning af udførelse" wrapText="true" />
                        <Label fx:id="lblLimitation" />
                     </children>
                  </VBox>
                  <VBox prefWidth="600.0">
                     <children>
                        <Label styleClass="SubHeaderText" text="Borgerens ønsker og mål" wrapText="true" />
                        <Label fx:id="lblGoals" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox prefWidth="1100.0">
               <children>
                  <Label styleClass="SubHeaderText" text="Observationer" />
                  <Label fx:id="lblObservations" />
               </children>
            </VBox>
         </children>
      </VBox>
   </content>
</TitledPane>
